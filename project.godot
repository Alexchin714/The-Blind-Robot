; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="The Blind Robot"
run/main_scene="res://Intros/Scene/Intro.tscn"
boot_splash/image="res://Art/tbr_theblindrobotThumbnail.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://Art/tbr_icon.png"

[audio]

default_bus_layout="res://Music/Audio.tres"

[autoload]

SceneTransition="*res://Scene/SceneTransition.tscn"
SaveSettings="*res://Script/saveSettings.gd"
GlobalSettings="*res://Script/globalSettings.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1024
window/size/test_height=578
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

environment/default_environment="res://Shader/default_env.tres"
